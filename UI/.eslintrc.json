{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
  
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": [
        "warn"
      ],
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "jsx-quotes": ["warn", "prefer-single"],
      "comma-dangle": "off",
      "react/jsx-filename-extension": "off"
    }
  }
  
  
  